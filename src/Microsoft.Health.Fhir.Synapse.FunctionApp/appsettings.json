{
    "fhirServer": {
        "serverUrl": "https://example.azurehealthcareapis.com",
        "version": "R4",
        "authentication": "None"
    },
    "dataLakeStore": {
        "storageUrl": "https://example.blob.core.windows.net"
    },
    "job": {
        "containerName": "fhir",
        "startTime": "1970-01-01",
        "resourceTypeFilters": [
        ]
    },
    "schema": {
        "schemaCollectionDirectory": "./schemas"
    },
    "arrow": {
        "readOptions": {
            "useThreads": true,
            "blockSize": 30485760
        },
        "writeOptions": {
            "writeBatchSize": 100,
            "compression": "SNAPPY"
        }
    },
    "scheduler": {
        "maxConcurrencyCount": 30
    }
  }
  